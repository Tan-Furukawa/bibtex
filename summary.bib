	keywords = {Eclogite, Thermobaric structure, Hydration, Metamorphic facies series, Sanbagawa},
	number = {1},
	pages = {95-126},
	title = {Thermobaric structure and metamorphic evolution of the Iratsu eclogite body in the Sanbagawa belt, central Shikoku, Japan},
	url = {https://www.sciencedirect.com/science/article/pii/S0024493704000027},
