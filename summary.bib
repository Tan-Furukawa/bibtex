	journal = {Journal of Geophysical Research: Solid Earth},
	number = {B12},
	pages = {30523-30546},
	title = {Effect of fine-scale microstructures in titanohematite on the acquisition and stability of natural remanent magnetization in granulite facies metamorphic rocks, southwest Sweden: Implications for crustal magnetism},
	url = {https://agupubs.onlinelibrary.wiley.com/doi/abs/10.1029/2001JB000180},
	volume = {106},
	year = {2001},
