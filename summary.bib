	number = {6},
	pages = {2391--2410},
	reading = {0},
	title = {Extended Larch{\'e}--Cahn framework for reactive Cahn--Hilliard multicomponent systems},
	url = {https://doi.org/10.1007/s00161-021-01045-9},
	volume = {33},
