	journal = {Journal of Geophysical Research: Solid Earth},
	keywords = {immiscibility, high pressure, silicate systems},
	number = {B3},
	title = {Effect of pressure on liquid-liquid miscibility gaps: A case study of the systems CaO-SiO2, MgO-SiO2, and CaMgSi2O6-SiO2},
