	issn = {0022-3530},
	journal = {Journal of Petrology},
	month = {10},
	number = {2},
	pages = {329-353},
	title = {{Zircon Crystal Morphology, Trace Element Signatures and Hf Isotope Composition as a Tool for Petrogenetic Modelling: Examples From Eastern Australian Granitoids}},
