	issn = {0956-540X},
	journal = {Geophysical Journal International},
	month = {03},
	number = {1},
	pages = {486-495},
	summary = {1},
	title = {{Magnetic properties of Archean gneisses from the northeastern North China Craton: the relationship between magnetism and metamorphic grade in the deep continental crust}},
