	keywords = {FFT-Based method, Discrete green operators, Eigenstrains, Eshelby inclusion, Inhomogeneties, Dislocations},
	pages = {1-23},
	title = {Development of a new consistent discrete green operator for FFT-based methods to solve heterogeneous problems with eigenstrains},
