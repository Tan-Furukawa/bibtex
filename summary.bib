	issn = {0003-004X},
	journal = {American Mineralogist},
	month = {12},
	number = {11-12},
	pages = {1413-1424},
	read = {1},
	reading = {1},
	title = {{Thermodynamics of order-disorder in minerals; I, Symmetric formalism applied to minerals of fixed composition}},
