	journal = {Journal of Geophysical Research: Solid Earth},
	keywords = {phase-field modeling, crystal size distribution (CSD), diffusion chronometry},
	note = {e2023JB026718 2023JB026718},
	number = {5},
	pages = {e2023JB026718},
	reading = {0},
	title = {Phase-Field Simulation of Texture Evolution in Magmatic Rocks},
	url = {https://agupubs.onlinelibrary.wiley.com/doi/abs/10.1029/2023JB026718},
