	journal = {International Journal for Numerical Methods in Engineering},
	keywords = {fast Fourier transforms, augmented Lagrangians, composite materials},
	number = {1-2},
	pages = {139-160},
	title = {A computational scheme for linear and non-linear composites with arbitrary phase contrast},
