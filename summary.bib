	journal = {Gondwana Research},
	keywords = {Lachlan fold belt, Bega Batholith, I-type granite, Zircon U-Pb geochronology, Crustal growth},
	pages = {1-19},
	title = {U-Pb geochronology of the Silurian-Devonian Bega Batholith, south-eastern Australia: Insights into the origin and development of I-type granites},
	url = {https://www.sciencedirect.com/science/article/pii/S1342937X22002106},
