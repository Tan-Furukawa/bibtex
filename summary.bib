@article{2001JB000180,
	abstract = {Mid-Proterozoic granulites in SW Sweden, having opaque minerals hematiteilmenite with minor magnetite, and occurring in an area with negative aeromagnetic anomalies, have strong and stable reversed natural remanent magnetization ∼9.2 A/m, with 100\% remaining after demagnetization to 100 mT. Samples were characterized by optical microscopy, electron microprobe (EMP), transmission electron microscopy (TEM), and rock-magnetic measurements. Earliest oxide equilibrium was between grains of titanohematite and ferri-ilmenite at 650$\,^{\circ}$--600$\,^{\circ}$C. Initial contacts were modified by many exsolution cycles. Hematite and ilmenite (Ilm) hosts and lamellae by EMP are Ilm 24--25, ILm 88--93, like titanohematite, and ilmenite above 520$\,^{\circ}$C on Burton's diagram [1991]. Finer hosts and lamellae by TEM are Ilm16 $\pm$3 and Ilm 88$\pm$4, like coexisting antiferromagnetically ordered (AF) hematite and ilmenite below 520$\,^{\circ}$C on Burton's diagram. This may be the first example of analytical identification, in one sample, of former hematite, now finely exsolved, and AF hematite. TEM microstructures consist of gently curving semicoherent ilmenite lamellae within hematite, flanked by precipitate-free zones and abundant ilmenite disks down to unit cell scale (1--2 nm). Strain contrast of disks suggests full coherence with the host, and probable formation at the reaction titanohematite ---> AF hematite + ilmenite at 520$\,^{\circ}$C. Magnetic properties are a consequence of chemical and magnetic evolution of hematite and ilmenite with bulk compositions ilmenite-richer than Ilm 28, that apparently exsolved without becoming magnetized, down to 520$\,^{\circ}$C where hematite broke down to AF hematite plus ilmenite, producing abundant AF hematite below its N{\'e}el temperature. Intensity of magnetization is greater than possible with hematite alone, and TEM work suggests that ultrafine ilmenite disks in AF hematite are associated with a ferrimagnetic moment due to local imbalance of up and down spins at coherent interfaces.},
	author = {McEnroe, Suzanne A. and Harrison, Richard J. and Robinson, Peter and Golla, Ute and Jercinovic, Michael J.},
	date-added = {2023-07-03 13:45:52 +0900},
	date-modified = {2023-07-03 13:45:52 +0900},
	doi = {https://doi.org/10.1029/2001JB000180},
