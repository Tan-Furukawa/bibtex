	issn = {0956-540X},
	journal = {Geophysical Journal International},
	month = {08},
	number = {2},
	pages = {479-488},
	tags = {magnetic},
	title = {{Palaeomagnetism of the Egersund-Ogna anorthosite, Rogaland, Norway, and the position of Fennoscandia in the Late Proterozoic}},
	url = {https://doi.org/10.1111/j.1365-246X.2004.02349.x},
	volume = {158},
