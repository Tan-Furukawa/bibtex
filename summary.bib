	keywords = {first-order reversal curves, FORCs, unmixing, principal component analysis, PCA, greigite},
	number = {5},
	pages = {1595-1610},
	title = {An Improved Algorithm for Unmixing First-Order Reversal Curve Diagrams Using Principal Component Analysis},
	url = {https://agupubs.onlinelibrary.wiley.com/doi/abs/10.1029/2018GC007511},
