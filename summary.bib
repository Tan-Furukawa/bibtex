	journal = {Earth-Science Reviews},
	keywords = {Equilibrium under non-hydrostatic stress, Chemical potential of stressed solid, Thermodynamic pressure, Phase rule under non-hydrostatic stress, Clapeyron slope under non-hydrostatic stress, Soft modes, Elastic softening, Plasticity and chemical equilibrium},
	pages = {190-233},
	tags = {nonequ},
	title = {Does non-hydrostatic stress influence the equilibrium of metamorphic reactions?},
