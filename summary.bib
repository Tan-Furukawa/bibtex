	isbn = {0973-774X},
	journal = {Journal of Earth System Science},
	number = {6},
	pages = {1159--1175},
	tags = {quartz},
	title = {Seismic properties of naturally deformed quartzites of the Alaknanda valley, Garhwal Himalaya, India},
	url = {https://doi.org/10.1007/s12040-015-0605-6},
