	keywords = {Microtomography, Radiography, 2D/3D image analysis, Technical binary alloys, LSW},
	number = {15},
	pages = {5045-5055},
	title = {Coarsening of grain-refined semi-solid Al--Ge32 alloy: X-ray microtomography and in situ radiography},
