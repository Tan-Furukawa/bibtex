	isbn = {1432-0967},
	journal = {Contributions to Mineralogy and Petrology},
	number = {10},
	pages = {102},
	read = {1},
	title = {Thermodynamics of ordering and mixing in plagioclase feldspars: atomistic modelling in favour of Landau theory},
	url = {https://doi.org/10.1007/s00410-022-01965-y},
	volume = {177},
	year = {2022},
